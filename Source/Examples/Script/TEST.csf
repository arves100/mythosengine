include "export.csf";

create drawable="Shower" type=particle maxcount=4096
{
    position=0,0,0;
    parts=50;
    life=4;
    speed=40;
    circle=5,45;
    color=255,0,0;
}

modify drawable="Cube"
{
    direction=0,0,1;
}

animate drawable="Cube"
{
    rotatey = 45;
}

animate drawable="Shower"
{
    rotatey = 30;
}

waitkey=10000;

